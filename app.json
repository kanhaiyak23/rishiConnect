{
  "expo": {
    "name": "RishiConnect",
    "slug": "rishiconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconf.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
     "scheme": "rishiconnect", 
    "splash": {
      "image": "./assets/iconf.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rishihood.rishiconnect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconfi.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rishihood.rishiconnect",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set your profile picture."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/iconf.png",
          "color": "#FF6B6B",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "3bec20d0-2bf8-4a0c-b2df-87b230a0ab26"
      }
    },
    "owner": "kanhaiya121"
  }
}
